{
    "manifest_version": 3,
    "name": "YouTube Progress Tracker",
    "version": "1.1",
    "description": "Track YouTube video progress and generate completion certificates",
    "permissions": [
      "storage",
      "activeTab"
    ],
    "background": {
    "service_worker":"Backend/background.js"
  },
    "action": {
      "default_popup": "Page.html"
    },
      "author": "dhanazx1@gmail.com",
  "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "host_permissions": [
      "*://*.youtube.com/*",
      "https://gemini.googleapis.com/"
    ]
  }
  